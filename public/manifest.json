{
    "manifest_version": 3,
    "name": "Highlight Saver",
    "version": "0.0.1",
    "description": "Highlight Saver",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ],
    "permissions": [
      "storage"
    ]
}